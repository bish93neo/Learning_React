    "chai": "*",
    "chai-jquery": "*",
    "jquery": "*",
    "jsdom": "*",
    "mocha": "*",
    "react-addons-test-utils": "*",
    "webpack": "*",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "loadash": "^1.0.0",
    "lodash": "*",
    "react": "^16.3.1",
    "react-dom": "*",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
